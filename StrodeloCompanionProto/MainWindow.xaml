<Window x:Class="StrodeloCompanionProto.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StrodeloCompanionProto"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="IpAddressBox" HorizontalAlignment="Left" Margin="165,83,0,0" TextWrapping="Wrap" Text="127.0.0.1" VerticalAlignment="Top" Width="253" Padding="5" FontSize="30"/>
        <Button x:Name="PairButton" Content="Pair" HorizontalAlignment="Left" Margin="488,83,0,0" VerticalAlignment="Top" Padding="20" Click="PairButton_Click" FontSize="30"/>
        <Button x:Name="SendFileButton" Content="Send file" HorizontalAlignment="Left" Margin="378,191,0,0" VerticalAlignment="Top" Padding="40" Click="SendFileButton_Click" FontSize="30"/>
    </Grid>
</Window>
